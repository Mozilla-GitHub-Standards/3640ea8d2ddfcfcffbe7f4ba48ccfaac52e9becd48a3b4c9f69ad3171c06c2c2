{
    "name": "ensemble-transposer",
    "version": "0.2.0",
    "private": true,
    "dependencies": {
        "babel-cli": "6.26.0",
        "babel-preset-env": "1.7.0",
        "redis": "2.8.0",
        "request": "2.87.0",
        "restify": "7.2.1",
        "restify-cors-middleware": "1.1.1"
    },
    "devDependencies": {
        "eslint": "5.1.0",
        "eslint-plugin-json": "1.2.0",
        "eslint-plugin-node": "7.0.1",
        "nodemon": "1.18.3",
        "npm-run-all": "4.1.3"
    },
    "engines": {
        "node": ">=8"
    },
    "scripts": {
        "start": "node build/server",
        "build": "npm-run-all build:*",
        "build:app": "babel src --source-maps --minified --out-dir build",
        "build:version.json": "node scripts/generateVersionJSON",
        "postinstall": "npm run build",
        "local": "npm-run-all --parallel local:*",
        "local:build-and-watch": "babel src --watch --source-maps --out-dir build",
        "local:start-and-watch": "nodemon build/server",
        "validate": "npm run test",
        "test": "npm run lint",
        "lint": "npm-run-all lint:*",
        "lint:js": "eslint --ext=.js,.json .",
        "posttest": "npm audit || true"
    }
}
