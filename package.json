{
    "name": "ensemble-transposer",
    "version": "0.2.0",
    "private": true,
    "engines": {
        "node": ">=8"
    },
    "scripts": {
        "start": "echo 'Run \"npm run dev\" or \"npm run prod\"'",
        "dev": "docker-compose -f docker-compose.base.yml -f docker-compose.dev.yml up",
        "prod": "docker-compose -f docker-compose.base.yml up",
        "build": "node scripts/generateVersionJSON",
        "validate": "npm run test",
        "test": "npm run lint",
        "lint": "eslint --ext=.js,.json .",
        "posttest": "npm audit || true"
    },
    "dependencies": {
        "cors": "2.8.4",
        "express": "4.16.3",
        "helmet": "3.13.0",
        "morgan": "1.9.0",
        "redis": "2.8.0",
        "request": "2.88.0"
    },
    "devDependencies": {
        "eslint": "5.3.0",
        "eslint-plugin-json": "1.2.1",
        "eslint-plugin-node": "7.0.1",
        "nodemon": "1.18.3"
    }
}
