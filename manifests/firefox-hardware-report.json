        },
        "titlemap": {
            "resolution": "Display Resolution",
            "gpuModel": "GPU Model",
            "gpuVendor": "GPU",
            "has": "Unity",
            "cpuSpeed": "CPU Speeds",
            "ram": "Memory",
            "cpuCores": "CPU Cores",
            "cpuVendor": "CPU",
            "osName": "Operating System",
            "browserArch": "Browsers by Architecture",
            "osArch": "OSs by Architecture",
            "hasFlash": "Flash"
        },
        "sections": [
            {
                "title": "Graphics",
                "charts": ["resolution", "gpuModel", "gpuVendor", "has"]
            },
            {
                "title": "Processor",
                "charts": ["cpuSpeed", "ram", "cpuCores", "cpuVendor"]
            },
            {
                "title": "Operating System",
                "charts": ["osName", "browserArch", "osArch"]
            },
            {
                "title": "Flash",
                "charts": ["hasFlash"]
            }
        ]
    }
