{
    "source": "https://s3-us-west-2.amazonaws.com/telemetry-public-analysis-2/usage_report_data_2/v1/master/webusage.json",
    "extraMetadata": {
        "title": "Usage Behavior",
        "defaultCategory": "Worldwide",
        "metrics": {
            "locale": {
                "title": "Top Languages",
                "description": "The distribution of the top 5 language settings.",
                "type": "line",
                "axes": {
                    "y": {
                        "unit": "%"
                    }
                }
            },
            "pct_TP": {
                "title": "Tracking Protection",
                "description": "The percentage of clients with Tracking Protection enabled.",
                "type": "line",
                "axes": {
                    "y": {
                        "unit": "%"
                    }
                }
            },
            "pct_addon": {
                "title": "Has Add-on",
                "description": "The percentage of clients with a user installed add-on.",
                "type": "line",
                "axes": {
                    "y": {
                        "unit": "%"
                    }
                }
            },
            "top10addons": {
                "title": "Top Add-ons",
                "description": "The top 10 most popular add-ons.",
                "type": "table",
                "columns": [
                    {
                        "name": "Add-on"
                    },
                    {
                        "name": "Usage",
                        "unit": "%"
                    }
                ]
            }
        },
        "dashboard": {
            "sectioned": false,
            "metrics": [
                "locale",
                "os",
                "pct_TP",
                "pct_addon",
                "name"
            ]
        }
    }
}
